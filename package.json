{
  "name": "tech-test",
  "version": "1.0.0",
  "description": "A node / typescript project for a technical test.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "cross-env BASE_URL=http://127.0.0.1:3001/api jest --config=./jest.config.e2e.json",
    "e2e:watch": "cross-env BASE_URL=http://127.0.0.1:3001/api jest --config=./jest.config.e2e.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ireoostacchini/tech-test.git"
  },
