  "bugs": {
    "url": "https://github.com/ireoostacchini/tech-test/issues"
  },
  "homepage": "https://github.com/ireoostacchini/tech-test#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/convict": "^5.2.1",
    "@types/method-override": "0.0.31",
    "@types/pg": "^7.14.4",
    "body-parser": "^1.19.0",
    "convict": "^6.0.0",
    "convict-format-with-validator": "^6.0.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
