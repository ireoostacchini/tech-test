    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.24",
    "axios": "^0.19.2",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
